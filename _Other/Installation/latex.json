{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }


	"Aligned as i like": {
        "prefix": "aligned",
        "body": [
            "\\\\[\\begin{aligned}",
            "    $0",
            "\\\\end{aligned}\\]"
        ],
        "description": "Insert aligned environment"
    },


	"\\[ \\]": {
        "prefix": "math",
        "body": [
        	"\\\\[$0\\\\]"
        ],
        "description": "Basic math environment"
    },

	"Minipage Environment": {
		"prefix": "minipage",
		"body": [
			"\\begin{minipage}{0.48\\textwidth}",
			"    $0",
			"\\end{minipage}"
		],
		"description": "Insert minipage environment"
	},

	"Subsection{}": {
		"prefix": "sub",
		"body": [
			"\\\\subsection{$0}"
		],
		"description": "Insert subsection"
	},

	"Subsubsection": {
		"prefix": "ssub",
		"body": [
			"\\\\subsubsection{$0}"
		],
		"description": "Insert subsubsection"
	},
	"Bold Text": {
    "prefix": "tbf",
    "body": [
        "\\\\textbf{$0}"
    ],
    "description": "Insert textbf"
	},


	"Includegraphics": {
		"prefix": "img",
		"body": [
			"\\\\includegraphics[width=0.9\\\\textwidth]{$0}"
		],
		"description": "Insert includegraphics with width=0.9\\textwidth"
	},

	"Text": {
    "prefix": "tx",
    "body": [
        "\\\\text{$0}"
    ],
    "description": "Insert text"
	},

	"Quad rightArrow quad": {
    "prefix": "qrq",
    "body": [
        "\\\\quad \\\\rightarrow \\\\quad"
    ],
    "description": "Insert \\quad \\rightarrow \\quad"
}
	
}